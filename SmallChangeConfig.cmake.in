set(@PROJECT_NAME@_VERSION       "@PROJECT_VERSION@")
set(@PROJECT_NAME@_VERSION_MAJOR "@PROJECT_VERSION_MAJOR@")
set(@PROJECT_NAME@_VERSION_MINOR "@PROJECT_VERSION_MINOR@")
set(@PROJECT_NAME@_VERSION_PATCH "@PROJECT_VERSION_PATCH@")

@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
include(GNUInstallDirs)

find_dependency(Coin)

if(@Qt5_FOUND@)
  find_dependency(Qt5Core)
  find_dependency(Qt5Widgets)
endif()

# include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Export.cmake")

set_and_check(@PROJECT_NAME@_INCLUDEDIR "@PACKAGE_SM_INCLUDEDIR@")
set_and_check(@PROJECT_NAME@_LIB_DIR    "@PACKAGE_SM_LIBDIR@")

# optional directory from documentation component
if (EXISTS "@PACKAGE_COIN_DOC_DIR@/")
  set(@PROJECT_NAME@_DOCDIR "@PACKAGE_SM_DOCDIR@")
endif(EXISTS @PACKAGE_COIN_DOC_DIR@)

set(@PROJECT_NAME@_LIBRARIES  "@PROJECT_NAME@")
